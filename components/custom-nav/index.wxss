.custom-nav {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  background-color: rgba(255, 255, 255, 0);
  overflow: hidden;
}

.custom-nav-content {
  display: flex;
}

.custom-nav-left {
  flex: 0 0 auto;
}

.menu-btn-placeholder {
  flex: 0 0 auto;
}

.custom-nav-title {
  flex: 1 1 auto;
  color: #fff;
  height: 100%;
  text-align: center;
  font-size: 14px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}